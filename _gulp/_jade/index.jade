    // Preloader
    //- include ./_include/_preloader

    include ./_include/_header

    // Section
    section.section.helps

      .helps__layer.scene
        .container
          img(src="./img/img-apple1.png", alt="" data-depth="0.40").layer.layer--1
          img(src="./img/img-apple2.png", alt="" data-depth="0.80").layer.layer--2
          img(src="./img/img-apple3.png", alt="" data-depth="0.60").layer.layer--3
          img(src="./img/img-banana1.png", alt="" data-depth="0.40").layer.layer--4

      .helps__layer.helps__layer--last.scene
        .container
          img(src="./img/img-strawberries1.png", alt="" data-depth="0.60").layer.layer--5

      .helps__wrapper
        .container

          img(src="./img/img-iphone1.png", alt="").iphone1

          .row
            .col-lg-7.col-md-6.col-sm-8
              a(href="#", target="_blank").appicon
                img(src="./img/img-appicon.png", alt="")

              .h1 Helps you eat&nbsp;
                strong better.
              p FitApp is a free calorie counting and diet app PSD template which includes high quality 9 screens. This template can be very helpful if you're planning to build food tracking application.

              .applinks
                a(href="#", target="_blank")
                  img(src="./img/icon-appstore.png", alt="")

                a(href="#", target="_blank")
                  img(src="./img/icon-google-play.png", alt="")

